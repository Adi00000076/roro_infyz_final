  "build:uat": "vite build --mode uat && javascript-obfuscator dist --output dist --config obfuscator.config.json",
  "build:prod": "vite build --mode production && javascript-obfuscator dist --output dist --config obfuscator.config.json",
  "preview": "vite preview",
  "preview:uat": "vite preview --mode uat",
  "preview:prod": "vite preview --mode production",
